<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane fx:id="ap" alignment="CENTER" hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.eu.passwallet.client.flexui.ManagePassWalletUIController">
    <children>
        <GridPane alignment="TOP_CENTER" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="TOP">
            <children>
                <FlowPane alignment="TOP_LEFT" GridPane.columnIndex="0" GridPane.rowIndex="0">
                    <children>
                    <Label alignment="TOP_LEFT" prefHeight="21.0" prefWidth="44.0" text="Filter:">
                     <FlowPane.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </FlowPane.margin>
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </padding></Label>
                    <TextField fx:id="filter" alignment="TOP_LEFT" onKeyReleased="#onFilterKeyReleased" prefHeight="25.0" prefWidth="258.0" text="">
                        <padding>
                            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                        </padding>
                     <FlowPane.margin>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </FlowPane.margin>
                    </TextField>
                    </children>
                </FlowPane>
                <GridPane alignment="TOP_LEFT" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
                    <children>
                        <FlowPane alignment="TOP_LEFT">
                            <children>
                            <Button mnemonicParsing="false" onAction="#onAdd" prefHeight="31.0" prefWidth="65.0" text="Add">
                                <padding>
                                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                </padding>
                           <FlowPane.margin>
                              <Insets bottom="2.0" left="2.0" right="10.0" top="2.0" />
                           </FlowPane.margin>
                            </Button>
                                <Button mnemonicParsing="false" onAction="#onEdit" prefHeight="31.0" prefWidth="62.0" text="Edit">
                           <padding>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </padding>
                           <FlowPane.margin>
                              <Insets bottom="2.0" left="2.0" right="20.0" top="2.0" />
                           </FlowPane.margin></Button>
                            <Button mnemonicParsing="false" onAction="#onDelete" prefHeight="31.0" prefWidth="64.0" text="Delete">
                                <padding>
                                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                </padding>
                           <FlowPane.margin>
                              <Insets bottom="2.0" left="2.0" right="10.0" top="2.0" />
                           </FlowPane.margin>
                            </Button>
                            <Button mnemonicParsing="false" onAction="#onCopy" prefHeight="31.0" prefWidth="65.0" text="Copy">
                                <padding>
                                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                </padding>
                           <FlowPane.margin>
                              <Insets bottom="2.0" left="2.0" right="10.0" top="2.0" />
                           </FlowPane.margin>
                            </Button>
                            </children>
                        </FlowPane>
                    </children>
               <columnConstraints>
                  <ColumnConstraints />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints />
               </rowConstraints>
                </GridPane>
            <Label id="passWalletFile" fx:id="passWalletFile" prefHeight="21.0" prefWidth="1178.0" text="Pass wallet file: " GridPane.rowIndex="2" />
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="35.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <padding>
                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
            </padding>
        </GridPane>
        <TableView id="usersAccounts" fx:id="usersAccounts" prefHeight="692.0" prefWidth="1234.0" GridPane.rowIndex="1">
            <columns>
                <TableColumn id="id" prefWidth="50.0" text="Id" />
                <TableColumn id="nickName" prefWidth="300.0" text="Nickname" />
                <TableColumn id="name" prefWidth="300.0" text="Name" />
                <TableColumn id="site" prefWidth="300.0" text="Site URL" />
                <TableColumn id="description" prefWidth="300.0" text="Description" />
            </columns>
            <padding>
                <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
            </padding>
        </TableView>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="100.0" vgrow="SOMETIMES" />
    </rowConstraints>
</GridPane>
